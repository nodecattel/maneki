{
  "name": "maneki",
  "version": "1.0.0",
  "description": "Helius webhook monitoring tool",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "tsx --no-warnings src/index.ts",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'tsx --no-warnings' src/index.ts",
    "build": "tsc",
    "clean": "rm -rf dist",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write 'src/**/*.ts'",
    "prepare": "husky install"
  },
  "dependencies": {
    "@solana/web3.js": "^1.91.0",
    "arweave": "^1.15.5",
    "axios": "^1.6.7",
    "csv-parse": "^5.5.6",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "fastestsmallesttextencoderdecoder": "^1.0.22",
    "helius-sdk": "^1.4.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.11",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "private": true,
  "keywords": [
    "solana",
    "helius",
    "webhook",
    "monitoring"
  ],
  "author": "n3k0h1m3",
  "license": "MIT"
}
